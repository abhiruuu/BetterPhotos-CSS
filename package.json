{
  "name": "betterphotos-css",
  "version": "1.0.0",
  "description": "A Photography Social Media App UI made using HTML, CSS and SCSS",
  "main": "index.js",
  "scripts": {
    "compile": "sass --watch --no-source-map src/main.scss src/style.css",
    "build:compile": "sass --no-source-map src/main.scss build/compiled.css",
    "build:prefixes": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/style.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "csso-cli": "^4.0.2",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "sass": "^1.83.1"
  }
}
